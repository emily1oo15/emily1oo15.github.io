@charset "utf-8"

html,body
{
	background-color:white;
}

table.schedule
{
	background: white;
	border: 10px outset rgb(153, 0, 153);
	border-collapse: collapse;
	font-size: 0.75em;
	width: 100%
}

table.schedule th, table.schedule td 
{
		border: 1px solid black;
}
	
table.schedule caption
{
	caption-side: bottom;
	text-align: right;
}
		